  {
   "cell_type": "code",
   "execution_count": 31,
   "metadata": {},
   "outputs": [],
   "source": [
    "demograph_link = 'https://en.wikipedia.org/wiki/Demography_of_London'\n",
    "headers = {'User-Agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64; rv:64.0) Gecko/20100101 Firefox/64.0'}\n",
    "demograph_page = requests.get(demograph_link, headers = headers)\n",
    "soup1 = BeautifulSoup(demograph_page.content, 'html.parser')\n",
    "table1 = soup1.find('table', {'class':'wikitable sortable'}).tbody\n",
    "rows1 = table1.find_all('tr')\n",
    "columns1 = [i.text.replace('\\n', '')\n",
    "           for i in rows1[0].find_all('th')]"
   ]
  },
